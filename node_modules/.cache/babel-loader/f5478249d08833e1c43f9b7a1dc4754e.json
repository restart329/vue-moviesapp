MAAA,OAAA,CAAA,GAAA,CAAA,GAAA,CAAA,IAAA;AACA,UAAA,GAAA,GAAA,GAAA,CAAA,IAAA,CAAA,GAAA;;AACA,UAAA,GAAA,KAAA,IAAA,EAAA;AACA,QAAA,KAAA,CAAA,QAAA,GAAA,GAAA,CAAA,IAAA,CAAA,IAAA,CAAA,KAAA;AACA,QAAA,KAAA,CAAA,SAAA,GAAA,KAAA;AACA,QAAA,KAAA,CAAA,UAAA,GAAA,MAAA,CAHA,CAIA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA,KAjDA;AAkDA;AAhFA,CAAA","sourcesContent":["<template>\r\n  <div class=\"movie_body\" ref=\"movie_body\">\r\n    <Loading v-if=\"isloading\"/>\r\n    <Scroller v-else>\r\n    <ul>\r\n        <li class=\"pullDown\">{{pullDowm}}</li>\r\n        <li v-for=\"data in datalist\" :key=\"data.filmId\">\r\n            <div class=\"pic_show\" @click=\"handleToDetail(data.filmId)\"><img :src=\"data.poster\"></div>\r\n                <div class=\"info_list\">\r\n                    <h2 @click=\"handleToDetail(data.filmId)\">{{ data.name }}\r\n                      <span>{{data.filmType.name}}</span>\r\n                    </h2>\r\n                    <p>观众评分： <span class=\"grade\">{{ data.grade }}</span></p>\r\n                    <p>主演: {{ data.actors | actorsfilter}}</p>\r\n                    <p>简介：{{ data.synopsis }}</p>\r\n                </div>\r\n                <div class=\"btn_mall\">\r\n                    购票\r\n            </div>\r\n        </li>\r\n    </ul>\r\n    </Scroller>\r\n</div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios'\r\nimport Vue from 'vue'\r\n// import BScroll from 'better-scroll'\r\nVue.filter('actorsfilter', function (data) {\r\n  var newlist = data.map(item => item.name)\r\n  return newlist.join(' ')\r\n})\r\nexport default {\r\n  name: 'NowPlaying',\r\n  data () {\r\n    return {\r\n      datalist: [],\r\n      pullDowm: '',\r\n      isloading: true,\r\n      prevCityId: -1\r\n    }\r\n  },\r\n  methods: {\r\n    handleToDetail (filmId) {\r\n      // console.log(filmId)\r\n      this.$router.push('/movie/detail/1/' + filmId)\r\n      // this.$router.push(`/detail/${id}`)\r\n      // this.$router.push({ name: 'filmdetail', params: { id: id } })\r\n    }\r\n  },\r\n  activated () {\r\n    // 跨域请求数据\r\n    // axios.get('/ajax/movieOnInfoList?token=&optimus_uuid=81542220255311EB9CA34791782AA9DB4C1697C66F3F40D48091E7BFF39725B5&optimus_risk_level=71&optimus_code=10').then(res => {\r\n    //   console.log(res.data)\r\n    // })\r\n    // 验证请求数据\r\n    var cityId = this.$store.state.city.id\r\n    if (this.prevCityId === cityId) {\r\n      return\r\n    }\r\n    this.isloading = true\r\n    console.log(123)\r\n    axios({\r\n      url: 'https://m.maizuo.com/gateway?cityId=110100&pageNum=1&pageSize=10&type=1&k=6613682',\r\n      headers: {\r\n        'X-Client-Info': '{\"a\":\"3000\",\"ch\":\"1002\",\"v\":\"5.0.4\",\"e\":\"1605253643172292613079041\",\"bc\":\"110100\"}',\r\n        'X-Host': 'mall.film-ticket.film.list'\r\n      }\r\n    }).then(res => {\r\n      console.log(res.data)\r\n      var msg = res.data.msg\r\n      if (msg === 'ok') {\r\n        this.datalist = res.data.data.films\r\n        this.isloading = false\r\n        this.prevCityId = cityId\r\n        // this.$nextTick(() => {\r\n        // /* eslint-disable no-new */\r\n        //   var scroll = new BScroll(this.$refs.movie_body, {\r\n        //     click: true,\r\n        //     tap: true,\r\n        //     probeType: 1\r\n        //   })\r\n        //   scroll.on('scroll', (pos) => {\r\n        //     console.log('scroll')\r\n        //     if (pos.y > 30) {\r\n        //       this.pullDowm = '正在更新中'\r\n        //     }\r\n        //   })\r\n        //   scroll.on('touchEnd', (pos) => {\r\n        //     console.log('touchend')\r\n        //     if (pos.y > 30) {\r\n        //       axios({\r\n        //         url: 'https://m.maizuo.com/gateway?cityId=110100&pageNum=1&pageSize=10&type=1&k=6613682',\r\n        //         headers: {\r\n        //           'X-Client-Info': '{\"a\":\"3000\",\"ch\":\"1002\",\"v\":\"5.0.4\",\"e\":\"1605253643172292613079041\",\"bc\":\"110100\"}',\r\n        //           'X-Host': 'mall.film-ticket.film.list'\r\n        //         }\r\n        //       }).then(res => {\r\n        //         var msg = res.data.msg\r\n        //         if (msg === 'ok') {\r\n        //           this.pullDowm = '更新成功'\r\n        //           setTimeout(() => {\r\n        //             this.datalist = res.data.data.films\r\n        //             this.pullDowm = ''\r\n        //           }, 1000)\r\n        //         }\r\n        //       })\r\n        //     }\r\n        //   })\r\n        // })\r\n      }\r\n    })\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n#content .movie_body{ flex:1; overflow:auto;}\r\n.movie_body ul{ margin:0 12px; overflow: hidden;}\r\n.movie_body ul li{ margin-top:12px; display: flex; align-items:center; border-bottom: 1px #e6e6e6 solid; padding-bottom: 10px;}\r\n.movie_body .pic_show{ width:64px; height: 90px;}\r\n.movie_body .pic_show img{ width:100%;}\r\n.movie_body .info_list { margin-left: 10px; flex:1; position: relative;}\r\n.movie_body .info_list h2{ font-size: 17px; line-height: 24px; width:150px; overflow: hidden; white-space: nowrap; text-overflow:ellipsis;position: relative;}\r\n.movie_body .info_list h2 span{\r\n    display: inline-block;\r\n    font-size: 9px;\r\n    padding:0px 2px ;\r\n    width: 18px;\r\n    line-height: 14px;\r\n    background-color: rgb(200, 211, 210);\r\n    color: white;\r\n    border-radius: 2px;\r\n    margin-left: 4px;\r\n    position: absolute;\r\n    top: 6px;\r\n}\r\n.movie_body .info_list p{ font-size: 13px; color:#666; line-height: 22px; width:200px; overflow: hidden; white-space: nowrap; text-overflow:ellipsis;}\r\n.movie_body .info_list .grade{ font-weight: 700; color: #faaf00; font-size: 15px;}\r\n.movie_body .info_list img{ width:50px; position: absolute; right:10px; top: 5px;}\r\n.movie_body .btn_mall , .movie_body .btn_pre{ width:47px; height:27px; line-height: 28px; text-align: center; background-color: #f03d37; color: #fff; border-radius: 4px; font-size: 12px; cursor: pointer;}\r\n.movie_body .btn_pre{ background-color: #3c9fe6;}\r\n.movie_body .pullDown{\r\n  margin: 0px;\r\n  padding: 0px;\r\n  border: none;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/components/NowPlaying"}]}